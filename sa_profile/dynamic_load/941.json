{
    "process": "pinauth",
    "systemability": [
        {
            "name": 941,
            "libpath": "libpinauthservice.z.so",
            "run-on-create": false,
            "distributed": false,
            "recycle-delaytime": 1,
            "dump_level": 1,
            "start-on-demand": {
                "param": [
                    {
                        "name":"useriam.startSa",
                        "value":"true",
                        "conditions" : [
                            {
                                "eventId" : "param",
                                "name" : "useriam.isCredentialChecked",
                                "value" : "true"
                            }
                        ]
                    },
                    {
                        "name":"bootevent.useriam.fwkready",
                        "value":"true"
                    }
                ]
            },
            "stop-on-demand": {
                "param": [
                    {
                        "name":"useriam.stopSa",
                        "value":"true"
                    }
                ]
            },
            "min_hdi_proxy_version": ["libpin_auth_proxy_3.0.z.so"]
        }
    ]
}